{
    "rewrites": [
      {
        "source": "/admin",
        "destination": "/index.html"
      },
      {
        "source": "/app",
        "destination": "/index.html"
      },
      {
        "source": "/create-appointments",
        "destination": "/index.html"
      },
      {
        "source": "/services",
        "destination": "/index.html"
      },
      {
        "source": "/services/:serviceType",
